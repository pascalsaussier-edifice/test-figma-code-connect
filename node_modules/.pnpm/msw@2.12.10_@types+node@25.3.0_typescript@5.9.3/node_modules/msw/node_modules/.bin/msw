#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules/msw/cli/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules/msw/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules/msw/cli/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules/msw/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/msw@2.12.10_@types+node@25.3.0_typescript@5.9.3/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.js" "$@"
else
  exec node  "$basedir/../../cli/index.js" "$@"
fi
