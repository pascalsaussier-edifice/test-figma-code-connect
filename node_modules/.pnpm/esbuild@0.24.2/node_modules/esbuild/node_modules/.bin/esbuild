#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules/esbuild/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules/esbuild/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules/esbuild/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules/esbuild/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/esbuild@0.24.2/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
