#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules/vitest/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules/vitest/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@2.1.9_@types+node@25.3.0_@vitest+ui@2.1.9_jsdom@25.0.1_msw@2.12.10_@types+node@25.3.0__umbowtrr5fxghqzzp34bygemjq/node_modules/vitest/vitest.mjs" "$@"
fi
