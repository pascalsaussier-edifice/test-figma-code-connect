#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules/turbo/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules/turbo/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules/turbo/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules/turbo/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/turbo@2.8.10/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/turbo" "$@"
else
  exec node  "$basedir/../../bin/turbo" "$@"
fi
