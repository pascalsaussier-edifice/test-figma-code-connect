#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules/storybook/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules/storybook/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules/storybook/bin/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules/storybook/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/storybook@8.4.5_prettier@3.8.1/node_modules:/Volumes/Work/edifice-frontend-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.cjs" "$@"
else
  exec node  "$basedir/../../bin/index.cjs" "$@"
fi
